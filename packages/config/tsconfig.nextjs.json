{
	"$schema": "https://json.schemastore.org/tsconfig",
	"extends": "./tsconfig.json",
	"compilerOptions": {
		"target": "ES2022",
		"lib": ["ES2022", "DOM", "DOM.Iterable"],
		"jsx": "preserve",
		"module": "ESNext",
		"moduleResolution": "bundler",
		"noEmit": true,
		"plugins": [{ "name": "next" }]
	}
}
