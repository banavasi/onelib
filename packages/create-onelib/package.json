{
	"name": "create-onelib",
	"version": "0.1.0",
	"description": "CLI to scaffold Onelib projects",
	"type": "module",
	"bin": {
		"create-onelib": "./dist/index.js"
	},
	"main": "./dist/index.js",
	"scripts": {
		"build": "tsc",
		"dev": "tsc --watch",
		"lint": "biome check .",
		"format": "biome check --write .",
		"check-types": "tsc --noEmit",
		"test": "vitest run",
		"clean": "rm -rf dist .turbo node_modules"
	},
	"devDependencies": {
		"@types/fs-extra": "^11.0.4",
		"typescript": "^5.8.0"
	},
	"dependencies": {
		"@clack/prompts": "^1.0.1",
		"@onelib/templates": "workspace:*",
		"fs-extra": "^11.3.3",
		"picocolors": "^1.1.1"
	}
}
